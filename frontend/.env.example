# Firebase Configuration Template
# Kopieer dit bestand naar .env en vul je echte Firebase waarden in

# Firebase API Key
NG_APP_FIREBASE_API_KEY=your-firebase-api-key-here

# Firebase Auth Domain
NG_APP_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com

# Firebase Database URL
NG_APP_FIREBASE_DATABASE_URL=https://your-project-default-rtdb.firebaseio.com

# Firebase Project ID
NG_APP_FIREBASE_PROJECT_ID=your-project-id

# Firebase Storage Bucket
NG_APP_FIREBASE_STORAGE_BUCKET=your-project.appspot.com

# Firebase Messaging Sender ID
NG_APP_FIREBASE_MESSAGING_SENDER_ID=123456789

# Firebase App ID
NG_APP_FIREBASE_APP_ID=1:123456789:web:your-app-id
